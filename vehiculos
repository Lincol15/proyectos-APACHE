    private void CargarDatos() {
        
      ArrayList<Auto> autos=new ArrayList<>();
        autos=l2.ListaAutos();
        DefaultTableModel bmw=(DefaultTableModel) jTable1.getModel();
        bmw.setNumRows(0);
        for (Auto auto: autos){
            bmw.addRow(new Object[]{
            auto.getMarca(),
            auto.getModelo(),
            auto.getColor(),
            auto.getPrecio(),
            auto.getAño(),
            });
        }
        
       
        }

    private void limpiarDatos() {
        txtMARCA.setText("");
        txtMODELO.setText("");
        txtCOLOR.setText("");
        txtPRECIO.setText("");
        txtAÑO.setText("");
    }
    }

    

